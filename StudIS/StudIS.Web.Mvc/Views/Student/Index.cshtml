@using StudIS.Web.Mvc.Models;
@model List<CourseViewModel>

<div class="container">
    <div class="page-header">
        <h1>Upisani predmeti</h1>
    </div>

    <div class="list-group">
        @foreach (var course in Model)
        {
            <a href="/Student/ScoreInfo/@course.Id" class="list-group-item">@course.Name (@course.NaturalIdentifier)</a>
        }

    </div>
</div>